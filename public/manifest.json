  "shortcuts": [
    {
      "name": "Client Registration",
      "short_name": "Clients",
      "description": "Register enterprise clients for cultural assessment",
      "url": "/client-registration",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Candidate Assessment",
      "short_name": "Candidates",
      "description": "Assess candidate cultural compatibility",
      "url": "/candidate-registration",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Job Posting",
      "short_name": "Jobs",
      "description": "Create job postings with cultural requirements",
